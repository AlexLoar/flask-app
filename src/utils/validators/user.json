{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-03/schema#",
  "properties": {
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "required": true
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "maxLength": 40,
      "required": true
    },
    "age": {
      "$id": "/properties/age",
      "type": "integer",
      "minimum": 1,
      "maximum": 99,
      "required": true
    },
    "phone": {
      "$id": "/properties/phone",
      "type": "string",
      "required": true
    },
    "location": {
      "$id": "/properties/location",
      "type": "string",
      "required": true
    }
  }
}